                        android:id="@+id/marinastart6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:text="Starts At"
                        android:textColor="#F1ECEC"
                        android:textSize="12sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/skilletiiamge"
                        app:layout_constraintTop_toBottomOf="@+id/Skilltitle"
                        app:layout_constraintVertical_bias="0.042" />

                    <TextView
                        android:id="@+id/Skilltitle"
                        android:layout_width="190dp"
                        android:layout_height="21dp"
                        android:layout_marginStart="12dp"

                        android:singleLine="true"
                        android:text="Skilletti"
                        android:textColor="#B7FFFFFF"
                        android:textSize="18sp"
                        android:textStyle="normal|italic"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/skilletiiamge"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.202" />

                    <Button
                        android:id="@+id/skilletibtn"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:background="#00FFFFFF"
                        app:icon="@drawable/shoppingcart"
                        app:iconGravity="start"
                        app:iconPadding="10dp"
                        app:iconSize="20dp"
                        app:iconTint="#00E673"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.951"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.857"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <ImageView
                        android:id="@+id/skilletiiamge"
                        android:layout_width="112dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/skiletti" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>
